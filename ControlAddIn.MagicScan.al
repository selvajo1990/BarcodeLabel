controladdin "Magic Scan"
{
    MaximumHeight = 1;
    MaximumWidth = 1;
    MinimumHeight = 1;
    MinimumWidth = 1;
    HorizontalShrink = true;
    VerticalShrink = true;
    RequestedHeight = 1;
    RequestedWidth = 1;
    StartupScript = './res/script/MagicScan.js';
    event MagicScan(Barcode: Text);
}